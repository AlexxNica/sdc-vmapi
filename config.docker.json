{
    "logLevel": "debug",
    "maxSockets": 100,
    "useVmAgent": true,
    "api": {
        "port": 80
    },
    "heartbeater": {
        "host": "rabbitmq.@@DNS_DOMAIN@@",
        "queue": "heartbeat.vmapi",
        "reconnect": 5,
        "concurrency": 50,
        "retry": {
            "initialDelay": 4000
        }
    },
    "cache": {
        "type": "redis",
        "host": "redis.@@DNS_DOMAIN@@"
    },
    "wfapi": {
        "forceMd5Check": true,
        "workflows": [
            "provision",
            "start",
            "stop",
            "reboot",
            "reprovision",
            "update",
            "destroy",
            "snapshot",
            "rollback",
            "delete-snapshot",
            "add-nics",
            "update-nics",
            "remove-nics"
        ],
        "url": "http://workflow.@@DNS_DOMAIN@@"
    },
    "cnapi": {
        "maxSockets": 10,
        "url": "http://cnapi.@@DNS_DOMAIN@@"
    },
    "imgapi": {
        "url": "http://imgapi.@@DNS_DOMAIN@@"
    },
    "napi": {
        "url": "http://napi.@@DNS_DOMAIN@@"
    },
    "papi": {
        "url": "http://papi.@@DNS_DOMAIN@@"
    },
    "moray": {
        "host": "moray.@@DNS_DOMAIN@@",
        "port": 2020,
        "connectTimeout": 200,
        "retry": {
            "retries": 2,
            "minTimeout": 500
        }
    }
}
